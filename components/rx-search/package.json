{
  "dependencies": {
    "browserify": "~4.1.5"
  },
  "devDependencies": {
    "browserify-shim": "^3.5.0",
    "chai": "~1.9.1",
    "grunt": "~0.4.2",
    "grunt-browserify": "~2.1.0",
    "grunt-contrib-connect": "^0.7.1",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-handlebars": "*",
    "grunt-contrib-stylus": "~0.11.0",
    "grunt-contrib-uglify": "^0.4.0",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-mocha": "~0.4.7",
    "mocha": "~1.18.2",
    "nib": "~1.0.1",
    "reactify": "^0.13.1"
  },
  "browserify": {
    "transform": [
      "reactify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": {
      "exports": "global:$"
    },
    "underscore": {
      "exports": "global:_"
    },
    "react": {
      "exports": "global:React"
    }
  }
}
